{
    "name": "vito-ui",
    "version": "0.0.1",
    "description": "Vito UI Library by React",
    "type": "module",
    "source": "./src/index.ts",
    "types": "./src/index.d.ts",
    "exports": {
        "require": "./dist/index.cjs",
        "default": "./dist/index.modern.js"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.modern.js",
    "unpkg": "./dist/index.umd.js",
    "files": [
        "dist",
        "src",
        "package.json"
    ],
    "scripts": {
        "build": "microbundle --jsx React.createElement --no-compress --format modern,cjs,umd"
    },
    "author": "Jorge Gomez",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "microbundle": "^0.15.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tslib": "^2.4.1",
        "typescript": "^4.9.4"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
